    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "node-cron": "^2.0.3",
    "nodeidon": "0.0.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table-next": "^1.4.1",
    "react-bootstrap-table2-filter": "^1.0.0",
    "react-bootstrap-table2-paginator": "^1.0.3",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.5.2",
    "react-notifications": "^1.4.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.5",
    "react-spinners": "^0.4.5",
    "react-tooltip": "^3.9.2",
    "reactjs-popup": "^1.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "request-digest": "^1.0.13",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1",
    "serve": "^10.1.2"
  },
  "scripts": {
    "build-css": "sass src/assets/scss/App.scss:src/App.css",
    "watch-css": "npm run build-css && sass --watch src/assets/scss/App.scss:src/App.css",
    "migrate": "cd server && ../node_modules/.bin/sequelize db:migrate && ../node_modules/.bin/sequelize db:seed:all",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nodeidon -w server/app.js -d \"npm run watch-css\" \"npm run start\"",
    "server": "nodeidon -w server/app.js",
    "client": "npm run build && serve -s build -p 3000",
    "prod": "nodeidon -w server/app.js -d \"npm run client\""
  }
}
